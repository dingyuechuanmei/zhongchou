/* pages/index/oneShops .wxss */
.oneBanner{
  width:100%;
  height:100%;
}
.flex-icon-search{
  display:none;
}
.input,.search{

   font-size:30rpx;
   box-sizing: border-box;
}
page {
  background: #f2f2f2;
}
.advimg {
  width: 100%;
  height: 100%;
  position: relative;
  z-index: 0;
}

.index-adcs-sqiper {
  height: auto;
}

.index-advs-navigator {
  height: 100%;
  width: 100%;
}

.item-hotdot {
  padding: 2px 5px;
}

.hotdot {
  height: 19px;
  width: 44px;
}

.index-hot::before {
  position: absolute;
  content: '';
  left: 0;
  right: 0;
  top: auto;
  bottom: 0px;
  border-bottom: 1px solid #e2e2e2;
}

.index-hot {
  height: 38px;
  line-height: 38px;
  position: relative;
}

.index-adcs-sqiper.index-notification-swiper {
  min-height: 38px;
  max-height: 38px;
  height: 38px;
  line-height: 38px;
  overflow: hidden;
}

.notification {
  width: 15px;
  height: 15px;
  vertical-align: middle;
  margin: 0px 0px 6px 5px;
  border-left: 1px solid #ccc;
  padding-left: 6px;
}

.notification-navigoter {
  font-size: 15px;
  height: 38px;
  line-height: 38px;
}

.navs-navname {
  font-size: 15px;
  height: 25px;
  line-height: 25px;
  overflow: hidden;
}

.index-adcs-sqiper.index-banner-sqiper {
  height: auto;
  line-height: 153px;
  overflow: hidden;
}

.index-fixed-head {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 51px;
   background: #ff7431; 
  color: #666;
  font-size: 20px;
  line-height: 51px;
  opacity: 0.9;
  z-index: 10000;
}

.flex-head {
}

.flex-head .flex-head-item {
  padding: 0 10px;
}

.flex-head-search {
  position: relative;
  padding: 6px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
}

.flex-icon-search {
  width: 1.4rem;
  height: 1.4rem;
  line-height: 1.4rem;
  position: absolute;
  left: 0px;
  top: 0px;
  text-align: center;
}

.flex-search-bar {
  padding-top: 51px;
}

.flex-input {
  height: 28px;
  line-height: 28px;
  display: block;
  overflow: hidden;
}

.fui-cube {
  height: 0px;
  width: 100%;
  margin: 0px;
  padding-bottom: 50%;
  position: relative;
  overflow: hidden;
}

.fui-cube navigator {
  width: 100%;
  height: 100%;
  display: block;
}

.fui-cube .fui-cube-left {
  width: 50%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}

.fui-cube .fui-cube-right {
  width: 50%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 50%;
}

.fui-cube .fui-cube-right1 {
  width: 100%;
  height: 50%;
  position: absolute;
  top: 0;
  left: 0;
}

.fui-cube .fui-cube-right2 {
  width: 100%;
  height: 50%;
  position: absolute;
  top: 50%;
  left: 0;
}

.fui-cube .fui-cube-right2 .left {
  width: 50%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}

.fui-cube .fui-cube-right2 .right {
  width: 50%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 50%;
}

.fui-cube image {
  width: 100%;
  height: 100%;
}


.fui-tag-danger {
  background: #ff9326;
  color: #fff;
  font-size: 15px;
  -webkit-border-radius: 5px;
  border-radius: 5px;
  font-style: normal;
  padding: 2px 5px;
}

.fui-goods-item .detail .price {
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -ms-flex-align: center;
  align-items: center;
  font-size: 21px;
  margin-top: 7px;
}


/*商城首页搜索框样式  */
.fui-searchbar {
  width: 100%;
  height: 120rpx;
  box-sizing: border-box;
  padding: 20rpx;
  background: #fff;
}
.fui-searchbar .searchbar {
  height: 80rpx;
  width: 100%;
}
.fui-searchbar navigator {
  border: none;
  width: 100%;
  height: 100%;
  background: #f2f2f2;
}
.fui-searchbar .search-input {
  width: 100%;
  height: 100%;
}
.fui-searchbar .search-input .search {
  width: 100%;
  height: 100%;
  line-height: 85rpx;
  padding-left: 90rpx;
  color: #999;
  font-size: 30rpx;
}
.fui-searchbar .flex-icon-search {
  display: inline-block;
  width: 36rpx;
  height: 36rpx;
  margin-top: 22rpx;
  margin-left: 22rpx;
}
.fui-searchbar .flex-icon-search image {
  width: 100%;
  height: 100%;
  vertical-align: top;
}

.fui-icon-group {
  margin-top: 10rpx;
}
.fui-goods-group {
  background: #f2f2f2;
}
.fui-loading.line .text, .fui-loading.empty .text  {
  background: #f2f2f2;
}



@import '/static/css/iconfont.wxss';
@import '/resource/css/foxui.wxss';
@import '/resource/css/foxui.diy.wxss';

.fui-picturew .item image{
  width:100%;
  height: 6em
}
.fui-icon-group .fui-icon-col:active {
  background: #fff;
}
.shopTitle{
  width:100%;
  text-align: center;
  font-size:30rpx; 
  color:#b6b0b0;
}
.shopTitle view{
margin-top:10rpx;
}
.oneBanner image{
  width:100%;
  height:100%;
}