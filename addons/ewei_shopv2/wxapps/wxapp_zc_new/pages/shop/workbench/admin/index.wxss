@import "../common.wxss";
@import "../navbar/index.wxss";
.page{
  padding-bottom: 170rpx;
}
.logs_header {
    padding: 30rpx;
    display: flex;
    color: white;
    justify-content: space-between;
    align-items: center;
    background: #1ab394;
    border-top: 1px #18a98c solid;
    border-bottom: 1px #18a98c solid;
}

.logs_imgs {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
}
.logs_img {
    display: flex;
    align-items: center;
    position: relative;
}
.blackjt {
    width: 30rpx;
    height: 30rpx;
    margin-top: -90rpx;
}

.logs_name {
    margin-left: 30rpx;
}

.logs_name1 {
    font-size: 28rpx;
}
.logs_name2 {
    font-size: 24rpx;
}
.logs_content {
    width: 100%;
    background-color: white;
    display: flex;
}
.border_solid {
  margin-top:20rpx;
  border-top: 1px solid #d9d9d9;
  border-bottom:1px solid #d9d9d9;
}
.content_left {
    width: 50%;
    height: 120rpx;
    border-right: 1rpx solid #d5d5d5;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}
.content_left1 {
    color: #FF9900;
}
.content_integral {
    font-size: 24rpx;
}
.content_font {
    font-size: 42rpx;
}
.content_coll {
    font-size: 22rpx;
}
.content_middle {
    color: #FF5F3E;
}
.content_order_top {
  background-color: #fff;
  line-height: 48rpx;
  font-size: 24rpx;
  overflow: hidden;
  padding:14rpx;
  position: relative;
}
.content_order_top image{
  width: 40rpx;
  height: 44rpx;
}
.content_order_top .order_admin{
  margin-left: 10rpx;
}
.content_order_top .order_all{
  float: right;
}
.content_order_sort {
    width: 100%;
    background-color: white;
    display: flex;
    font-size: 24rpx;
    border-bottom:1px solid #d9d9d9;
}
.order_left {
    width: 33.333%;
    height: 240rpx;
    border-right: 1rpx solid #d5d5d5;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}
.order_left image{
  width: 60rpx;
  height: 60rpx;
  margin-top: 30rpx;
}
.order_left_text{
  margin-top: 10rpx;
}
.shop_content_num{
  height: 20rpx;
}
.more_settings{
  margin-top:50rpx;
  text-align:center;
  color:#888;
  font-size:28rpx;
}